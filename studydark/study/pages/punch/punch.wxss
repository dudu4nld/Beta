}


.title{
  display: flex;
  flex-direction: column;
  align-items: center;  
}
.img{
    width: 300rpx;
    height: 300rpx;
    border-radius: 50%;
    margin: 100rpx 0;
}

.rankpace{
  color: #87CEFA;
}
.circle{
    left: 50rpx;
    bottom: 200rpx;         
    width: 260rpx;
    background-color: #87CEFA;
    height: 260rpx;
    padding-top: 70rpx;
    color: white;
    background-size: cover;
    position: fixed;
    z-index: 9999;
    font-size: 47rpx;
    text-align: center;
    border: 0 solid #ffffff;
    border-radius: 500rpx;
    box-shadow: 4px 1px 1px #cccccc;
}

